{
  "name": "owl-question-box",
  "version": "0.1.0",
  "description": "Box that can receive questions from owl-twitch-chat or owl-twitter-search",
  "homepage": "",
  "author": {
    "name": "bitowl",
    "email": "bitowl@bitowl.de",
    "url": "https://bitowl.net"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "bundleDependencies": {
      "owl-nodecg-styles": "^0.0.0"
    },
    "dashboardPanels": [
      {
        "name": "question-box",
        "title": "Question Box",
        "width": 3,
        "file": "question-box.html",
        "headerColor": "#525F78"
      },
      {
        "name": "delete-question",
        "title": "Delete Question?",
        "width": 2,
        "file": "delete-question.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      },
      {
        "name": "delete-all-questions",
        "title": "Delete all questions?",
        "width": 2,
        "file": "delete-all-questions.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}